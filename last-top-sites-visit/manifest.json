{
  "name": "TopVisits",
  "version": "0.0.1",
  "description": "List of the last top sites visited.",
  "manifest_version": 2,
  "permissions": [
    "topSites"
  ],
  "browser_action": {
    "default_title": "TopVisits",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/16.png",
      "32": "icons/32.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
    }
  }
}